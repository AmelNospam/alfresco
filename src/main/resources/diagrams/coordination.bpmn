          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              execution.setVariable('mwf_coordOutcome', task.getVariable('mwf_coordOutcome'));
              if(task.getVariable('mwf_coordOutcome')=='Reject'){
                execution.setVariable('mwf_lastCommentText', task.getVariable('bpm_comment'));
                execution.setVariable('mwf_lastCommentCreator', person.properties['cm:firstName']+ ' ' +person.properties['cm:lastName']);    
                execution.setVariable('mwf_lastCommentTaskType', "Согласование ЮО");        
              }                          
            </activiti:string>    
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="coordinationFOTask" name="Согласование ФО" activiti:candidateGroups="${mwf_financiersGroup}" activiti:formKey="mwf:coordinationFOTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              if (typeof bpm_workflowDueDate != 'undefined') task.setVariable('bpm_dueDate', bpm_workflowDueDate);
              if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
               execution.setVariable('mwf_coordOutcome', task.getVariable('mwf_coordOutcome'));           
              if(task.getVariable('mwf_coordOutcome')=='Reject'){
                execution.setVariable('mwf_lastCommentText', task.getVariable('bpm_comment'));
                execution.setVariable('mwf_lastCommentCreator', person.properties['cm:firstName']+ ' ' +person.properties['cm:lastName']);    
                execution.setVariable('mwf_lastCommentTaskType', "Согласование ФО");    
              }
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="coordinationEndTask" name="Договор согласован" activiti:assignee="${bpm_assignee.properties.userName}" activiti:formKey="mwf:coordinationEndTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              if (typeof bpm_workflowDueDate != 'undefined') task.setVariable('bpm_dueDate', bpm_workflowDueDate);
              if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="coordinationStartTask" targetRef="createContractTask"></sequenceFlow>
    <sequenceFlow id="flow2" sourceRef="createContractTask" targetRef="coordinationHeadTask"></sequenceFlow>
    <sequenceFlow id="flow12" sourceRef="coordinationEndTask" targetRef="endevent1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow13" sourceRef="coordinationHeadTask" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow14" sourceRef="exclusivegateway1" targetRef="coordinationUOTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow15" sourceRef="exclusivegateway1" targetRef="createContractTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow16" sourceRef="coordinationUOTask" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow17" sourceRef="exclusivegateway3" targetRef="coordinationFOTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" sourceRef="exclusivegateway3" targetRef="createContractTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow19" sourceRef="coordinationFOTask" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow20" sourceRef="exclusivegateway2" targetRef="coordinationEndTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" sourceRef="exclusivegateway2" targetRef="createContractTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${mwf_coordOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
  </process>
