<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:omgdc="http://www.omg.org/spec/DD/20100524/DC" xmlns:omgdi="http://www.omg.org/spec/DD/20100524/DI" typeLanguage="http://www.w3.org/2001/XMLSchema" expressionLanguage="http://www.w3.org/1999/XPath" targetNamespace="http://www.activiti.org/test">
  <process id="coordination" name="coordination" isExecutable="true">
    <extensionElements>
      <activiti:executionListener event="start" class="org.alfresco.repo.workflow.activiti.listener.ScriptExecutionListener">
        <activiti:field name="script">
          <activiti:string>
              execution.setVariable('mwf_managerschiefGroup', groups.getGroup('managers_chief').getFullName());
            execution.setVariable('mwf_financiersGroup', groups.getGroup('financiers').getFullName());
            execution.setVariable('mwf_lawGroup', groups.getGroup('law').getFullName());
          </activiti:string>
        </activiti:field>k
      </activiti:executionListener>
    </extensionElements>
    <startEvent id="coordinationStartTask" name="Старт процесса согласования" activiti:formKey="mwf:coordinationStartTask">    
    </startEvent>
    <userTask id="createContractTask" name="Подготовка текста договора" activiti:assignee="${bpm_assignee.properties.userName}" activiti:formKey="mwf:createContractTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              if (typeof bpm_workflowDueDate != 'undefined') task.setVariable('bpm_dueDate', bpm_workflowDueDate);
              if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              var head = task.getVariable('mwf_head');
              execution.setVariable('mwf_head', head.properties.userName);
              execution.setVariable('mwf_lastCommentText', task.getVariable('bpm_comment'));
              execution.setVariable('mwf_lastCommentCreator', person.properties['cm:firstName']+ ' ' +person.properties['cm:lastName']);    
              execution.setVariable('mwf_lastCommentTaskType', 'Подготовка текста договора');                    
            </activiti:string>          
          </activiti:field>
        </activiti:taskListener>        
      </extensionElements>
    </userTask>
    <userTask id="coordinationHeadTask" name="Согласование Руководитель" activiti:assignee="${mwf_head}" activiti:formKey="mwf:coordinationHeadTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              if (typeof bpm_workflowDueDate != 'undefined') task.setVariable('bpm_dueDate', bpm_workflowDueDate);
              if (typeof bpm_workflowPriority != 'undefined') task.priority = bpm_workflowPriority;
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              execution.setVariable('mwf_coordOutcome', task.getVariable('mwf_coordOutcome'));
              if(task.getVariable('mwf_coordOutcome')=='Reject'){
                execution.setVariable('mwf_lastCommentText', task.getVariable('bpm_comment'));
                execution.setVariable('mwf_lastCommentCreator', person.properties['cm:firstName']+ ' ' +person.properties['cm:lastName']);    
                execution.setVariable('mwf_lastCommentTaskType', "Согласование Руководитель");    
              }          
            </activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <userTask id="coordinationUOTask" name="Согласование ЮО" activiti:candidateGroups="${mwf_lawGroup}" activiti:formKey="mwf:coordinationUOTask">
      <extensionElements>
        <activiti:taskListener event="create" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string>
              if (typeof bpm_workflowDueDate != 'undefined') task.setVariable('bpm_dueDate', bpm_workflowDueDate);
